	"tests": [
		{
			"name": "Test to get IP",
			"base": "httpbin.org",
			"auth": "noAuth",
			"path": "/ip",
			"method": "GET",
			"query": {},
			"header": {},
			"data": {},
			"assert": {
				"type": "json",
				"code": 200,
				"fields": {
					"origin": "string"
				}
			}
		},
		{
			"name": "Test to POST data",
			"base": "httpbin.org",
			"auth": "noAuth",
			"path": "/post",
			"method": "POST",
			"query": {},
			"header": {},
			"data": {
				"posttest": "return to post"
			},
			"assert": {
				"type": "json",
				"code": 200,
				"fields": {
					"data": "string",
					"json": {
						"posttest": "string"
					}
				}
			}
		},
		{
			"name": "Test not found 404",
			"base": "httpbin.org",
			"auth": "noAuth",
			"path": "/status/404",
			"method": "GET",
			"query": {},
			"header": {},
			"data": {},
			"assert": {
				"code": 404
			}
		},
		{
			"name": "Test status teapot",
			"base": "httpbin.org",
			"auth": "noAuth",
			"path": "/status/418",
			"method": "GET",
